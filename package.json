{
  "name": "koa2-template",
  "version": "1.0.0",
  "description": "My best practice of building Koa2 server project with TypeScript",
  "scripts": {
    "dev": "nodemon -e ts app",
    "dev:ws": "nodemon -e ts wsApp",
    "start": "NODE_ENV=production ts-node app --port $PORT",
    "start:ws": "NODE_ENV=production ts-node wsApp --port $PORT",
    "prd": "pm2 start bin/www",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "consola": "^3.1.0",
    "ejs": "^3.1.9",
    "koa": "^2.14.2",
    "koa-body": "^6.0.1",
    "koa-router": "^12.0.0",
    "koa-send": "^5.0.1",
    "koa-static": "^5.0.0",
    "koa-views": "^8.0.0",
    "koa-websocket": "^7.0.0",
    "koa2-cors": "^2.0.6"
  },
  "devDependencies": {
    "@types/koa": "^2.13.6",
    "@types/koa-multer": "^1.0.1",
    "@types/koa-router": "^7.4.4",
    "@types/koa-static": "^4.0.2",
    "@types/koa2-cors": "^2.0.2",
    "@types/node": "^20.2.5",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
